{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Express.js template",
  "main": "server.js",
  "scripts": {
    "dev": "node --exec babel-node server.js",
    "serve": "nodemon --exec babel-node server.js",
    "format": "prettier server.js **/*.js --write",
    "test": "NODE_ENV=test jest --testTimeout=10000"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "mongoose": "^5.10.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.0.0",
    "@shelf/jest-mongodb": "^1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "jest": "^26.4.0",
    "nodemon": "^1.18.3",
    "prettier": "^2.0.5",
    "superagent": "^6.0.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
